<launch>
  <arg name="first_tb3"  default="tb3_0"/>
  <arg name="second_tb3" default="tb3_1"/>
  <arg name="third_tb3"  default="tb3_2"/>
  <arg name="3_tb3"  default="tb3_3"/>
  <arg name="4_tb3"  default="tb3_4"/>
  <arg name="5_tb3"  default="tb3_5"/>
  <arg name="6_tb3"  default="tb3_6"/>
  <arg name="7_tb3"  default="tb3_7"/>
  <arg name="8_tb3"  default="tb3_8"/>
  <arg name="9_tb3"  default="tb3_9"/>
  <arg name="10_tb3"  default="tb3_10"/>
  <arg name="11_tb3"  default="tb3_11"/>
  <arg name="12_tb3"  default="tb3_12"/>
  <arg name="13_tb3"  default="tb3_13"/>
  <arg name="14_tb3"  default="tb3_14"/>


  <arg name="first_tb3_x_pos" default="-7.0"/>
  <arg name="first_tb3_y_pos" default="-1.0"/>
  <arg name="first_tb3_z_pos" default=" 0.0"/>
  <arg name="first_tb3_yaw"   default=" 0.0"/>

  <arg name="second_tb3_x_pos" default=" 7.0"/>
  <arg name="second_tb3_y_pos" default="-1.0"/>
  <arg name="second_tb3_z_pos" default=" 0.0"/>
  <arg name="second_tb3_yaw"   default=" 0.0"/>

  <arg name="third_tb3_x_pos" default=" 0.5"/>
  <arg name="third_tb3_y_pos" default=" 3.0"/>
  <arg name="third_tb3_z_pos" default=" 0.0"/>
  <arg name="third_tb3_yaw"   default=" 0.0"/>

  <arg name="3_tb3_x_pos" default=" 0.0"/>
  <arg name="3_tb3_y_pos" default=" 0.0"/>
  <arg name="3_tb3_z_pos" default=" 0.0"/>
  <arg name="3_tb3_yaw"   default=" 0.0"/>

  <arg name="4_tb3_x_pos" default=" 0.0"/>
  <arg name="4_tb3_y_pos" default=" 0.0"/>
  <arg name="4_tb3_z_pos" default=" 0.0"/>
  <arg name="4_tb3_yaw"   default=" 0.0"/>

  <arg name="5_tb3_x_pos" default=" 0.0"/>
  <arg name="5_tb3_y_pos" default=" 0.0"/>
  <arg name="5_tb3_z_pos" default=" 0.0"/>
  <arg name="5_tb3_yaw"   default=" 0.0"/>

  <arg name="6_tb3_x_pos" default=" 0.0"/>
  <arg name="6_tb3_y_pos" default=" 0.0"/>
  <arg name="6_tb3_z_pos" default=" 0.0"/>
  <arg name="6_tb3_yaw"   default=" 0.0"/>

  <arg name="7_tb3_x_pos" default=" 0.0"/>
  <arg name="7_tb3_y_pos" default=" 0.0"/>
  <arg name="7_tb3_z_pos" default=" 0.0"/>
  <arg name="7_tb3_yaw"   default=" 0.0"/>

  <arg name="8_tb3_x_pos" default=" 0.0"/>
  <arg name="8_tb3_y_pos" default=" 0.0"/>
  <arg name="8_tb3_z_pos" default=" 0.0"/>
  <arg name="8_tb3_yaw"   default=" 0.0"/>

  <arg name="9_tb3_x_pos" default=" 0.0"/>
  <arg name="9_tb3_y_pos" default=" 0.0"/>
  <arg name="9_tb3_z_pos" default=" 0.0"/>
  <arg name="9_tb3_yaw"   default=" 0.0"/>

  <arg name="10_tb3_x_pos" default=" 0.0"/>
  <arg name="10_tb3_y_pos" default=" 0.0"/>
  <arg name="10_tb3_z_pos" default=" 0.0"/>
  <arg name="10_tb3_yaw"   default=" 0.0"/>

  <arg name="11_tb3_x_pos" default=" 0.0"/>
  <arg name="11_tb3_y_pos" default=" 0.0"/>
  <arg name="11_tb3_z_pos" default=" 0.0"/>
  <arg name="11_tb3_yaw"   default=" 0.0"/>

  <arg name="12_tb3_x_pos" default=" 0.0"/>
  <arg name="12_tb3_y_pos" default=" 0.0"/>
  <arg name="12_tb3_z_pos" default=" 0.0"/>
  <arg name="12_tb3_yaw"   default=" 0.0"/>


  <group ns="$(arg first_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg first_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg first_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg first_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg first_tb3_yaw)"  />
  </group>

  <group ns="$(arg second_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg second_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg second_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg second_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg second_tb3_yaw)"  />
  </group>

  <group ns="$(arg third_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg third_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg third_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg third_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg third_tb3_yaw)"  />
  </group>

  <group ns="$(arg 3_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 3_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 3_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 3_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 3_tb3_yaw)"  />
  </group>

  <group ns="$(arg 4_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 4_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 4_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 4_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 4_tb3_yaw)"  />
  </group>

  <group ns="$(arg 5_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 5_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 5_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 5_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 5_tb3_yaw)"  />
  </group>

  <group ns="$(arg 6_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 6_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 6_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 6_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 6_tb3_yaw)"  />
  </group>

  <group ns="$(arg 7_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 7_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 7_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 7_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 7_tb3_yaw)"  />
  </group>

  <group ns="$(arg 8_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 8_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 8_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 8_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 8_tb3_yaw)"  />
  </group>

  <group ns="$(arg 9_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 9_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 9_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 9_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 9_tb3_yaw)"  />
  </group>

  <group ns="$(arg 10_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 10_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 10_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 10_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 10_tb3_yaw)"  />
  </group>

  <group ns="$(arg 11_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 11_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 11_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 11_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 11_tb3_yaw)"  />
  </group>

  <group ns="$(arg 12_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg 12_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg 12_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg 12_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg 12_tb3_yaw)"  />
  </group>


  <node pkg="multirobot_map_merge" type="map_merge" respawn="false" name="map_merge" output="screen">
    <param name="robot_map_topic" value="map"/>
    <param name="robot_namespace" value="tb3"/>
    <param name="merged_map_topic" value="map"/>
    <param name="world_frame" value="map"/>
    <param name="known_init_poses" value="true"/>
    <param name="merging_rate" value="0.5"/>
    <param name="discovery_rate" value="0.05"/>
    <param name="estimation_rate" value="0.1"/>
    <param name="estimation_confidence" value="1.0"/>
  </node>

  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg first_tb3)_tf_broadcaster"  args="0 0 0 0 0 0 /map /$(arg first_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg second_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg second_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg third_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg third_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 3_tb3)_tf_broadcaster"  args="0 0 0 0 0 0 /map /$(arg 3_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 4_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg 4_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 5_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg 5_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 6_tb3)_tf_broadcaster"  args="0 0 0 0 0 0 /map /$(arg 6_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 7_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg 7_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 8_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg 8_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 9_tb3)_tf_broadcaster"  args="0 0 0 0 0 0 /map /$(arg 9_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 10_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg 10_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 11_tb3)_tf_broadcaster" args="0 0 0 0 0 0 /map /$(arg 11_tb3)/map 100"/>
  <node pkg="tf" type="static_transform_publisher" name="world_to_$(arg 12_tb3)_tf_broadcaster"  args="0 0 0 0 0 0 /map /$(arg 12_tb3)/map 100"/>
</launch>

